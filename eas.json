{
  "cli": {
    "version": ">= 10.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "$API_BASE_URL"  
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "API_BASE_URL": "$API_BASE_URL"  
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "API_BASE_URL": "$API_BASE_URL"
      }
    }
  },
